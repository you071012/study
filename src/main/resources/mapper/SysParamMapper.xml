<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ukar.study.mapper.SysParamMapper" >

    <sql id="Base_Column_List" >
    id, param_key, param_value, remark, create_time, update_time
    </sql>
    <select id="selectByParamKey" resultType="com.ukar.study.entity.SysParamDO">
        select <include refid="Base_Column_List"/> from sys_param where param_key = #{paramKey}

    </select>


</mapper>